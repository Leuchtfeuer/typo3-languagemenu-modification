{
  "name": "bitmotion/languagemod",
  "description": "Disables languages in HMENU when there is no translated record for given detail page.",
  "keywords": [
    "TYPO3",
    "Language",
    "Menu"
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.bitmotion.de",
  "authors": [
    {
      "name": "Florian Wessels",
      "email": "typo3-ext@bitmotion.de",
      "role": "Developer",
      "homepage": "https://www.bitmotion.de"
    }
  ],
  "support": {
    "issues": "https://github.com/bitmotion/typo3-languagemenu-modification/issues",
    "source": "https://github.com/bitmotion/typo3-languagemenu-modification",
    "email": "typo3-ext@bitmotion.de",
    "docs": "https://docs.typo3.org/p/bitmotion/languagemod/master/en-us/"
  },
  "require": {
    "typo3/cms-core": "^9.5",
    "typo3/cms-backend": "^9.5",
    "typo3/cms-extbase": "^9.5",
    "typo3/cms-frontend": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Bitmotion\\Languagemod\\": "Classes/"
    }
  },
  "replace": {
    "bitmotion/languagemod": "self.version",
    "typo3-ter/languagemod": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "languagemod"
    }
  }
}
